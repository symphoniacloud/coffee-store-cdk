{
    "name": "coffee-store-v2",
    "version": "2022.2.0",
    "homepage": "https://github.com/symphoniacloud/coffee-store-v2",
    "license": "MIT",
    "author": {
        "email": "mike@symphonia.io",
        "name": "Mike Roberts",
        "url": "https://symphonia.io"
    },
    "devDependencies": {
        "@aws-sdk/client-cloudformation": "3.x",
        "@jest/globals": "27.x",
        "@tsconfig/node16": "1.x",
        "@types/adm-zip": "0.x",
        "@types/aws-lambda": "8.x",
        "@types/glob": "7.x",
        "@types/jest": "27.x",
        "@types/node": "16.x",
        "aws-cdk": "2.x",
        "aws-cdk-lib": "2.x",
        "constructs": "10.x",
        "esbuild": "*",
        "jest": "27.x",
        "ts-jest": "27.x",
        "ts-node": "10.x",
        "typescript": "4.x"
    },
    "scripts": {
        "build": "tsc",
        "test": "npm run unit-test && npm run integration-test",
        "unit-test": "jest --config __tests__/jest-unit.config.js",
        "integration-test": "jest --config __tests__/jest-integration.config.js",
        "deploy": "npx cdk deploy --output build/cdk.out --require-approval never",
        "cdk-diff" : "npx cdk diff --output build/cdk.out --require-approval never",
        "cdk-destroy" : "npx cdk destroy --output build/cdk.out --require-approval never",
        "precdk-command": "echo \"Running CDK with command: $npm_config_command\"",
        "cdk-command": "npx cdk $npm_config_command --output build/cdk.out --require-approval never"
    }
}
